bg:Domain a owl:Class ;
    rdfs:label "Domain" ;
    rdfs:comment "A thematic or conceptual category for board games." ;
    rdfs:subClassOf skos:Concept .

bg:bggRank a owl:DatatypeProperty ;
    rdfs:label "bggRank" ;
    rdfs:comment "BoardGameGeek rank." ;
    rdfs:domain bg:BoardGame ;
    rdfs:range xsd:integer .

bg:complexityAverage a owl:DatatypeProperty ;
    rdfs:label "complexityAverage" ;
    rdfs:comment "Average complexity rating." ;
    rdfs:domain bg:Ratings ;
    rdfs:range xsd:decimal .

bg:hasDomain a owl:ObjectProperty ;
    rdfs:label "hasDomain" ;
    rdfs:comment "Relates a board game to one or more domains (as concepts)." ;
    rdfs:domain bg:BoardGame ;
    rdfs:range skos:Concept .

bg:hasMechanic a owl:ObjectProperty ;
    rdfs:label "hasMechanic" ;
    rdfs:comment "Relates a board game to one or more mechanics (as concepts)." ;
